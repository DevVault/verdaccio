{
  "name": "@verdaccio/hooks",
  "version": "6.0.0-6-next.11",
  "description": "loaders logic",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "build/types/index.d.ts",
  "author": {
    "name": "Juan Picado",
    "email": "juanpicado19@gmail.com"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/verdaccio/verdaccio"
  },
  "license": "MIT",
  "homepage": "https://verdaccio.org",
  "keywords": [
    "private",
    "package",
    "repository",
    "registry",
    "enterprise",
    "modules",
    "proxy",
    "server",
    "verdaccio"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "dependencies": {
    "@verdaccio/core": "workspace:6.0.0-6-next.4",
    "@verdaccio/logger": "workspace:6.0.0-6-next.9",
    "core-js": "3.20.3",
    "debug": "4.3.3",
    "handlebars": "4.7.7",
    "undici": "4.12.2",
    "undici-fetch": "1.0.0-rc.4"
  },
  "devDependencies": {
    "@types/node": "16.11.21",
    "@verdaccio/auth": "workspace:6.0.0-6-next.17",
    "@verdaccio/config": "workspace:6.0.0-6-next.12",
    "@verdaccio/types": "workspace:11.0.0-6-next.10"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'build/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'build/cjs' --source-maps",
    "build:js": "concurrently \"pnpm run build:cjs\" \"pnpm run build:esm\"",
    "build": "pnpm run build:js && pnpm run build:types"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/verdaccio"
  }
}
