{
  "name": "@verdaccio/cli",
  "version": "6.0.0-6-next.27",
  "author": {
    "name": "Juan Picado",
    "email": "juanpicado19@gmail.com"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "build/types/index.d.ts",
  "repository": {
    "type": "https",
    "url": "https://github.com/verdaccio/verdaccio"
  },
  "homepage": "https://verdaccio.org",
  "keywords": [
    "private",
    "package",
    "repository",
    "registry",
    "enterprise",
    "modules",
    "proxy",
    "server",
    "verdaccio"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "description": "verdaccio CLI",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./build",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules ",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'build/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'build/cjs' --source-maps",
    "build:js": "concurrently \"pnpm run build:cjs\" \"pnpm run build:esm\"",
    "build": "pnpm run build:js && pnpm run build:types",
    "start": "ts-node src/index.ts"
  },
  "dependencies": {
    "@verdaccio/core": "workspace:6.0.0-6-next.4",
    "@verdaccio/config": "workspace:6.0.0-6-next.12",
    "@verdaccio/logger": "workspace:6.0.0-6-next.9",
    "@verdaccio/node-api": "workspace:6.0.0-6-next.26",
    "@verdaccio/fastify-migration": "workspace:6.0.0-6-next.18",
    "clipanion": "3.1.0",
    "envinfo": "7.8.1",
    "kleur": "3.0.3",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "ts-node": "10.4.0"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/verdaccio"
  }
}
