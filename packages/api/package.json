{
  "name": "@verdaccio/api",
  "version": "6.0.0-6-next.20",
  "description": "backend api",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "build/types/index.d.ts",
  "author": {
    "name": "Juan Picado",
    "email": "juanpicado19@gmail.com"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/verdaccio/verdaccio"
  },
  "homepage": "https://verdaccio.org",
  "keywords": [
    "private",
    "package",
    "repository",
    "registry",
    "enterprise",
    "modules",
    "proxy",
    "server",
    "verdaccio"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'build/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'build/cjs' --source-maps",
    "build:js": "concurrently \"pnpm run build:cjs\" \"pnpm run build:esm\"",
    "build": "pnpm run build:js && pnpm run build:types",
    "watch": "pnpm build:js -- --watch"
  },
  "license": "MIT",
  "dependencies": {
    "@verdaccio/auth": "workspace:6.0.0-6-next.17",
    "@verdaccio/config": "workspace:6.0.0-6-next.12",
    "@verdaccio/core": "workspace:6.0.0-6-next.4",
    "@verdaccio/hooks": "workspace:6.0.0-6-next.11",
    "@verdaccio/logger": "workspace:6.0.0-6-next.9",
    "@verdaccio/middleware": "workspace:6.0.0-6-next.17",
    "@verdaccio/store": "workspace:6.0.0-6-next.18",
    "@verdaccio/utils": "workspace:6.0.0-6-next.10",
    "abortcontroller-polyfill": "1.7.3",
    "cookies": "0.8.0",
    "debug": "4.3.3",
    "body-parser": "1.19.1",
    "express": "4.17.2",
    "lodash": "4.17.21",
    "mime": "2.6.0",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "@types/node": "16.11.21",
    "@verdaccio/server": "workspace:6.0.0-6-next.25",
    "@verdaccio/types": "workspace:11.0.0-6-next.10",
    "@verdaccio/helper": "1.0.0",
    "supertest": "6.2.2"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/verdaccio"
  }
}
